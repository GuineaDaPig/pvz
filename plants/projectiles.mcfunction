
###############################################################################################################################
# Desc: Projectiles are triggered here as well as moving down the lane
###############################################################################################################################

execute at @e[tag=Pea] as @e[tag=Pea,distance=..1,limit=1] run function pvz:plants/projectiles/pea

execute at @e[tag=Gold_Pea] as @e[tag=Gold_Pea,distance=0] run function pvz:plants/projectiles/gold_pea

execute at @e[tag=LemonSeed] as @e[tag=LemonSeed,distance=0] run function pvz:plants/projectiles/lemon_seed

execute at @e[tag=LimeSeed] as @e[tag=LimeSeed,distance=0] run function pvz:plants/projectiles/lime_seed

execute at @e[tag=Shroom] as @e[tag=Shroom,distance=0] run function pvz:plants/projectiles/shroom_bullet

execute at @e[tag=Sprout] as @e[tag=Sprout,distance=0] run function pvz:plants/projectiles/sprout_bullet

execute at @e[tag=PomegranadeSeed] as @e[tag=PomegranadeSeed,distance=0] run function pvz:plants/projectiles/pome_seed

execute at @e[tag=burp] as @e[tag=burp,distance=0] run function pvz:plants/projectiles/burp

execute at @e[tag=Wind] as @e[tag=Wind,distance=0] run function pvz:plants/projectiles/wind










